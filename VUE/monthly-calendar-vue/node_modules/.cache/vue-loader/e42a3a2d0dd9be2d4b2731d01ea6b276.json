 operation {{ counter * 2 > 10 ? 'Greater than 10': 'Smaller than 10'}}-->\n</template>\n\n<script>\n\nexport default {\n  name: 'Events',\n\n  data() {\n    return {\n      counter: 0,\n    }\n  },\n\n  methods: {\n    increase() {\n      this.counter++;\n    },\n\n    updateCoordinates(event) {\n      console.log(event);\n      this.x = event.clientX;\n      this.y = event.clientY;\n    },\n\n    alert() {\n      alert('Alert!!!!');\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n\n<template>\n  <div>\n  <h1>{{message}}</h1>\n    <input type=\"text\" v-model=\"toDo\">\n    <button v-if=\"list.length < 10\" @click=\"addToDo\" >Add new todo</button>\n    <div>\n      <ul>\n        <li v-for=\"(toDo, index ) in list\" :key=\"index\" >\n          {{toDo}} <button @click=\"removeToDo(index)\" >Remove</button>\n        </li>\n      </ul>\n    </div>\n\n</div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\ndata(){\n    return{\n      message: 'ToDo',\n      list: ['Meting with client', 'Go to gym', 'Visit Mum' ],\n      toDo: ''\n    }\n},\n  methods:{\n    addToDo(){\n    if(this.toDo){\n    \n     this.list.push(this.toDo)\n        this.toDo = ''\n    }\n    },\n    removeToDo(index){\n   this.list = this.list.filter((el,i)  => i !== index)\n    }\n  }\n}\n</script>\n\n<style scoped>\ndiv{\n text-align: center;\n}\nul {\n  display: inline-block;\n  text-align: left;\n  font-size: 20px;\n}\nbutton{\n  margin-left: 20px;\n}\nli{\nmargin: 10px;\n}\n</style>"]}]}